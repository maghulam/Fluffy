pipeline {
  agent any
  stages {
    stage('Fluffy Build') {
      steps {
        echo 'Fluffy Build 1'
      }
    }

    stage('Fluffy Test') {
      steps {
        sleep 5
        echo 'Fluffy test 1'
      }
    }

    stage('Fluffy Deploy') {
      steps {
        echo 'Fluffy Deploy 1'
      }
    }

  }
}